
[MASTER]
ignore=CVS
ignore-patterns=
persistent=yes
load-plugins=

[MESSAGES CONTROL]
disable=
    missing-docstring,
    invalid-name,
    too-few-public-methods,
    too-many-arguments,
    too-many-instance-attributes,
    too-many-locals,
    too-many-branches,
    too-many-statements,
    too-many-return-statements,
    too-many-public-methods,
    fixme,
    line-too-long,
    wrong-import-order,
    wrong-import-position,
    ungrouped-imports,
    no-member,
    attribute-defined-outside-init,
    protected-access,
    unused-argument,
    unused-variable
